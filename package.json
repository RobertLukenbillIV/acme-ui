{
  "name": "acme-ui",
  "version": "1.0.0",
  "description": "A comprehensive React UI components library with navigation, forms, cards, tabbed interfaces, hero sections, and dark mode support. Built for modern web applications with accessibility and responsive design.",
  "main": "dist/acme-ui.umd.js",
  "module": "dist/acme-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/acme-ui.es.js",
      "require": "./dist/acme-ui.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.demo.config.js",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.js --emptyOutDir",
    "preview": "vite preview --config vite.demo.config.js",
    "preview:demo": "vite preview --config vite.demo.config.js --outDir demo-dist",
    "deploy": "npm run build:demo && vercel --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "library",
    "navigation",
    "forms",
    "cards",
    "tabs",
    "tabbed-card",
    "hero",
    "forum",
    "gallery",
    "dark-mode",
    "responsive",
    "accessibility",
    "typescript-ready"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-jest": "^30.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "vite": "^7.1.10"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
